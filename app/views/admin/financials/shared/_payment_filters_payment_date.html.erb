<span class="preserve-alignment">
  <label for="q_created_at_date_gteq">Filter by Date</label><br>
  <%= datetime_field_tag "q[created_at_date_gteq]", start_date, class: 'datepicker' %><a href="#q_created_at_date_gteq" class="btn btn--end calendar-toggle" title="Select a date"><i class="font-icon icon-calendar"></i></a>
</span>
<span class="preserve-alignment">
  <label for="q_created_at_date_lteq" class="is-invisible">End Date</label><br>
  <%= datetime_field_tag "q[created_at_date_lteq]", end_date, class: 'datepicker' %><a href="#q_created_at_date_lteq" class="btn btn--end calendar-toggle" title="Select a date"><i class="font-icon icon-calendar"></i></a>
</span>
<span class="preserve-alignment">
  <br>
  <%= f.submit "Filter", class: "btn--small" %>
</span>
