<% if @errors and @errors.keys.map{|k| @errors[k]["Errors"]}.select{|er| !er.empty?}.any? %>
	<h3>Errors</h3>
<% end %>
<div id="results">
<% if @errors %>
	<% @errors.each do |e| %>
		<% if @errors.has_key?("File") %>
			<%= @errors["File"] %>
		<% elsif not e[1]["Errors"].empty? %>
			<strong><%= "Row #{e[1]["Row number"]}:" %></strong><br>
			<% e[1]["Errors"].keys.each do |er| %>
				&nbsp;&nbsp;&nbsp; <%= "- #{er} (#{e[1]['Errors'][er]})" %>
				<br>
			<% end %>
		<% end %>
		<br></br>
	<% end %>
<% end %>
</div>