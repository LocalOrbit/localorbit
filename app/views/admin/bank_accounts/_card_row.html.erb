<tr class="bank-account">
  <td class="bank-name"><%= card.bank_name %></td>
  <td class="name"><%= card.name %></td>
  <td class="account-number">**** **** **** <%= card.last_four %></td>
  <td class="account-type">Credit Card</td>
  <td class="expiration">Expires <%= "%02d" % card.expiration_month %>/<%= card.expiration_year %></td>
  <td class="notes"><%= card.notes %></td>
  <td class="delete"><%= link_to raw("<i class='font-icon icon-delete'></i> Delete"), [:admin, @entity, card], data: { method: :delete } %></td>
</tr>
