<div class="sub-header stickable">
  <div class="l-constraint">
    <h1>Your account is not yet active</h1>
  </div>
</div>

<div class="sub-header-whitespace"></div>

<% if current_market.organization_inactive_note.present? %>
  <p><%= current_market.organization_inactive_note.html_safe %></p>
<% else %>
