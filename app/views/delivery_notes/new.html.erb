<div class="row row--partial">
  <div class="column column--three-fourths">
    <legend>Add Delivery Note</legend>


    <%= form_for :delivery_note do |f| %>
      <%= render 'form', {f: f} %>
      <div class="form-actions">
        <%= link_to "Cancel", cart_path(@cart), class: "btn cancel" %>
        <%= f.submit "Save Note" %>
      </div>
    <% end %>
  </div>
</div>