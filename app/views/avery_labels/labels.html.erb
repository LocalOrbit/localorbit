<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <%= render partial: "shared/print_opensans" %>
    <%= render partial: "shared/print_opensanscondensed_bold" %>
    <%= render partial: "shared/print_opensanscondensed_light" %>
  </head>
  <body style='margin: 0; padding: 0;'>
  <style>
    .page{
      page-break-after: always;
      font-family: "Open Sans Condensed";
      line-height: 110%;
      margin-left: .25in;
      margin-top: 50px;
      padding-top:50px;
      font-size: 16px;
      min-height: 11in;
      max-height: 11in;
      min-width: 8.5in;
      max-width: 8.5in;
      position: relative;
      overflow:hidden;
      white-space: nowrap;
    }
    .page-quarter {
      min-width: 395px;
      max-width: 395px;
      min-height: 510px;
      max-height: 510px;
      overflow: hidden;
      float: left;
    }
    .page-fifteenth {
        min-width: 2.625in;
        max-width: 2.625in;
        min-height: 2in;
        max-height: 2in;
        overflow: hidden;
        float: left;
        border: 1px solid #EEE;
        margin-right: .125in;
    }
    .label-box {
      min-width: 375px;
      max-width: 375px;
      min-height: 472px;
      max-height: 472px;
      overflow: hidden;
    }
    .upper-label { padding-top:  45px; }
    .middle-label { padding-top:  8px; }
    .lower-label { padding-top:  8px; }
    .left-label  { margin-left: 22px; }
    .right-label { margin-left: 5px; }

    .horizontal {
      -webkit-transform: rotate(-90deg);
      margin-left: -50px;
      margin-top: 60px;
      width: 462px;
      height: 365px;
    }
    .horizontal .label-row {
      width: 100%;
      margin-bottom: 2px;
      float: left;
    }
    .order-row {
      width: 100%;
      margin-bottom: 20px;
      float: left;
    }
    .label-content {

      padding: 1%;
      font-weight: 700;
    }
    .label-heading {
      text-align: center;
      background-color: #EEE;
      color: #222;
      padding: 8px;
      padding-top: 10px;
      font-weight: 800;
      font-size: 14px;
      width: 100%;
      margin-bottom: 20px;
      float: left;
    }
    .label-row {
      width: 100%;
      margin-bottom: 4px;
      float: left;
    }
    .small-label {
      font-size: 80%;
      font-weight: 400;
    }
    .small-text {
        font-size: 80%;
    }
    .footer-row {
      margin-top: 10px;
    }
    .footer-row img {
      max-height: 110px;
      max-width: 110px;
    }
    .left-code {
      float: left;
        width: 1.1in;
    }
    .left-code-qr {
        float: left;
        width: 1.9in;
    }
    .right-code {
      float: left;
        margin-left: 5px;
        border-left: 1px solid #CCC;
        padding-left: 5px;
        width: 1.5in;
    }
    .right-code-qr {
        float: left;
        width: 1.4in;
    }
    .right-code-qr img {
        max-width: 50px;
        max-height: 50px;
    }
    .qr-code {
    }
  </style>
  <% params[:pages].each do |page| %>
    <div class="page">
      <div class="label-row">
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:a][:template], locals: page[:a][:data] if page[:a] %>
          </div>
        </div>
        <div class="page-fifteenth">
            <div class="label-box">
              <%= render partial: page[:b][:template], locals: page[:b][:data] if page[:b] %>
            </div>
        </div>
        <div class="page-fifteenth">
            <div class="label-box">
              <%= render partial: page[:c][:template], locals: page[:c][:data] if page[:c] %>
            </div>
        </div>
      </div>
      <div class="label-row">
        <div class="page-fifteenth">
            <div class="label-box">
              <%= render partial: page[:d][:template], locals: page[:d][:data] if page[:d] %>
            </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:e][:template], locals: page[:e][:data] if page[:e] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:f][:template], locals: page[:f][:data] if page[:f] %>
          </div>
        </div>
      </div>
      <div class="label-row">
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:g][:template], locals: page[:g][:data] if page[:g] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:h][:template], locals: page[:h][:data] if page[:h] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:i][:template], locals: page[:i][:data] if page[:i] %>
          </div>
        </div>
      </div>
      <div class="label-row">
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:aj][:template], locals: page[:j][:data] if page[:j] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:k][:template], locals: page[:k][:data] if page[:k] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:l][:template], locals: page[:l][:data] if page[:l] %>
          </div>
        </div>
      </div>
      <div class="label-row">
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:m][:template], locals: page[:m][:data] if page[:m] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:n][:template], locals: page[:n][:data] if page[:n] %>
          </div>
        </div>
        <div class="page-fifteenth">
          <div class="label-box">
            <%= render partial: page[:o][:template], locals: page[:o][:data] if page[:o] %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% end %>
</body>
</html>
